{
  "name": "@trixta/trixta-js",
  "version": "3.3.7-beta.0",
  "description": "Javascript library to integrate Trixta",
  "main": "lib/bundle.cjs.js",
  "module": "lib/bundle.esm.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trixtateam/trixta-js.git"
  },
  "keywords": [
    "trixta",
    "react",
    "redux",
    "redux-saga",
    "phoenix-channels",
    "reselect",
    "node",
    "js",
    "javascript",
    "reaction",
    "space",
    "action"
  ],
  "author": "jacqueswho <jacqueswho@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trixtateam/trixta-js/issues"
  },
  "homepage": "https://github.com/trixtateam/trixta-js#readme",
  "scripts": {
    "dev": "rollup -c --watch",
    "link:phoenix-to-redux": "npm link @trixta/phoenix-to-redux",
    "build": "rollup -c",
    "prebuild": "rimraf lib",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "prepare": "npm run build",
    "major": "npm --no-git-tag-version version premajor --preid=beta",
    "patch": "npm --no-git-tag-version version prepatch --preid=beta",
    "minor": "npm --no-git-tag-version version preminor --preid=beta",
    "test:clean": "rimraf ./coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:staged": "jest --findRelatedTests"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "engines": {
    "node": ">=6"
  },
  "peerDependencies": {
    "lodash": "4.17.19",
    "reselect": "4.0.0",
    "moment": "^2.27.0",
    "immer": "3.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "redux-saga": "1.0.2",
    "redux": "4.0.1",
    "react-redux": "7.2.1",
    "shortid": "^2.2.15",
    "@trixta/phoenix-to-redux": "3.4.2-beta.0"
  },
  "dependencies": {
    "@trixta/phoenix-to-redux": "3.4.2-beta.0",
    "immer": "3.0.0",
    "lodash": ">=4",
    "moment": "^2.27.0",
    "phoenix": "1.5.4",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.2.1",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-redux-saga": "^1.1.2",
    "coveralls": "^3.1.0",
    "eslint": "7.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-redux-saga": "1.1.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-cli": "23.6.0",
    "lerna-alias": "^3.0.2",
    "pre-commit": "1.2.2",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "5.0.0-alpha.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.13",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-watch": "^4.3.1"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
