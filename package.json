{
  "name": "@trixta/trixtajs",
  "version": "1.0.0-beta.1",
  "description": "Javascript library to integrate Trixta",
  "main": "lib/bundle.cjs.min.js",
  "module": "lib/bundle.esm.min.js",
  "browser": "lib/bundle.umd.min.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trixtateam/trixtaJS.git"
  },
  "keywords": [
    "trixta",
    "react",
    "redux",
    "redux-saga",
    "phoenix-channels",
    "reselect",
    "node",
    "js",
    "javascript",
    "reaction",
    "space",
    "action"
  ],
  "author": "jacqueswho <jacqueswho@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trixtateam/trixtaJS/issues"
  },
  "homepage": "https://github.com/trixtateam/trixtaJS#readme",
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rimraf lib",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "engines": {
    "node": ">=6"
  },
  "peerDependencies": {
    "lodash": "4.17.19",
    "reselect": "4.0.0",
    "immer": "3.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "redux-saga": "1.0.0",
    "redux": "4.0.1",
    "react-redux": "7.2.1",
    "@trixta/phoenix-to-redux": "2.0.0-beta.1"
  },
  "dependencies": {
    "immer": "3.0.0",
    "lodash": ">=4",
    "phoenix": "1.5.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "prop-types": "^15.7.2",
    "reselect": "4.0.0",
    "redux-saga": "1.0.0",
    "redux": "4.0.1",
    "react-redux": "7.2.1",
    "@trixta/phoenix-to-redux": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "7.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-redux-saga": "1.1.3",
    "husky": "^4.2.5",
    "pre-commit": "1.2.2",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-terser": "^7.0.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
